<table class="table table-bordered">
    <thead>
    <tr>
        <th>Link Name</th>
        <th>Short URL</th>
    </tr>
    </thead>
    <tbody>
    <% @urls.each do |url| %>
    <tr>
    <td><%= url.name %></td>
    <td><%= link_to url.short_url, url.long_url %></td>
    </tr>
    <% end %>  
    </tbody>
</table>
<%= link_to 'Create another link', new_url_path %>       